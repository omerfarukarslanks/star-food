{
  "name": "sidebar",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/sidebar/src",
  "prefix": "star-food",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/sidebar/ng-package.json",
        "tailwindConfig": "libs/sidebar/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/sidebar/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/sidebar/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/sidebar/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/sidebar/**/*.ts", "libs/sidebar/**/*.html"]
      }
    }
  },
  "tags": ["type:sidebar"]
}
